    "version": "0.2.0",
    "configurations": [
        {
            "name": "datasetrun",
            "type": "python",
            "request": "launch",
            "program": "dataset.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            // "logToFile": true
        },
        {
            "name": "train",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            // "logToFile": true
            "args": [
                "--save-dir", "myoutdir",
                "--batch-size", "16",
                "--lr", "0.001",
                "--lr-step", "1"
            ]
        }
    ]
}